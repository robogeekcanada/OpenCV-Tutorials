   ],
   "source": [
    "#Let's play with rotation angle. We will clear the canvas first\n",
    "\n",
    "img[:,:] = WHITE\n",
    "\n",
    "thickness = 3\n",
    "for angle in range (0, 361, 30):\n",
    "   cv2.ellipse(img, center, axes, angle, start_angle, end_angle, RED, thickness)\n",
    "\n",
    "display_image(img, \"Ellipse Rotation\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
